                    $<span style="color: #009900;">&#123;</span>escapejs<span style="color: #009900;">&#40;</span>items.<span style="color: #660066;">U_ITEM</span><span style="color: #009900;">&#41;</span><span style="color: #009900;">&#125;</span><span style="color: #339933;">,</span>
