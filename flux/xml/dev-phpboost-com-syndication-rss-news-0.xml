                # ENDIF #
            # ENDIF #
            # IF items.<span style="color: #660066;">C_LOGO</span> #
                <span style="color: #000066; font-weight: bold;">var</span> logo <span style="color: #339933;">=</span> $<span style="color: #009900;">&#123;</span>escapejs<span style="color: #009900;">&#40;</span>items.<span style="color: #660066;">U_LOGO</span><span style="color: #009900;">&#41;</span><span style="color: #009900;">&#125;</span><span style="color: #339933;">;</span>
